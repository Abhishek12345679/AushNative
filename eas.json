{
    "cli": {
        "version": ">= 0.47.0"
    },
    "build": {
        "development": {
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleDevelopmentDebug"
            },
            "ios": {
                "buildConfiguration": "Debug"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk",
                "image": "ubuntu-18.04-jdk-11-ndk-r19c"
            }
        },
        "preview2": {
            "distribution": "internal",
            "android": {
                "node": "16.13.0",
                "image": "ubuntu-18.04-jdk-11-ndk-r19c"
            }
        },
        "production": {
            "android": {
                "gradleCommand": ":app:bundleProductionRelease"
            }
        }
    },
    "submit": {
        "production": {}
    }
}